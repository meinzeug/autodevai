{
  "name": "autodevai",
  "version": "1.0.0",
  "description": "AutoDev AI - Automated Development Platform",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "start": "npm run dev",
    "security:scan": "node scripts/security/security-monitor.js scan",
    "security:fix": "node scripts/security/security-monitor.js fix",
    "security:monitor": "node scripts/security/security-monitor.js monitor",
    "security:assess": "node scripts/security/pr-merger-integration.js assess",
    "security:merge": "node scripts/security/pr-merger-integration.js merge",
    "pipeline:validate": "./scripts/pipeline-validator.sh",
    "pipeline:validate:quick": "./scripts/pipeline-validator.sh --quick",
    "pipeline:security": "./scripts/security-scanner.sh --report",
    "pipeline:security:fix": "./scripts/security-scanner.sh --fix",
    "pipeline:pr:merge": "node ./scripts/pr-merger.js",
    "pipeline:pr:analyze": "node ./scripts/pr-merger.js --dry-run",
    "pipeline:docs": "./scripts/doc-updater.sh",
    "pipeline:docs:roadmap": "./scripts/doc-updater.sh --roadmap-only",
    "pipeline:monitor": "python3 ./scripts/pipeline-monitor.py",
    "pipeline:notify": "node ./scripts/notification-manager.js",
    "pipeline:rollback:status": "./scripts/rollback-manager.sh status",
    "pipeline:rollback:snapshot": "./scripts/rollback-manager.sh create-snapshot",
    "pipeline:rollback:list": "./scripts/rollback-manager.sh list-snapshots",
    "pipeline:health": "./scripts/rollback-manager.sh auto-recovery"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.6.0",
    "@tauri-apps/plugin-updater": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@octokit/rest": "^20.0.0",
    "@tauri-apps/cli": "^1.6.2",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jsdom": "^25.0.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.47",
    "postcss-custom-properties": "^14.0.6",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typedoc": "^0.26.0",
    "vite": "^5.4.10",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meinzeug/autodevai.git"
  },
  "keywords": [
    "ai",
    "automation",
    "development",
    "security",
    "tauri",
    "react",
    "typescript"
  ],
  "author": "AutoDev AI Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
