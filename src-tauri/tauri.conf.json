{
  "identifier": "com.meinzeug.autodev-ai",
  "productName": "AutoDev-AI Neural Bridge",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build && ./src-tauri/before-build.sh",
    "devUrl": "http://localhost:50010",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "AutoDev-AI Neural Bridge",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "center": true,
        "minWidth": 1200,
        "minHeight": 700,
        "maximized": false,
        "visible": true,
        "decorations": true,
        "alwaysOnTop": false,
        "fullscreen": false,
        "shadow": true,
        "transparent": false,
        "skipTaskbar": false
      },
      {
        "label": "dev-tools",
        "title": "AutoDev-AI Developer Tools",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "center": true,
        "visible": false,
        "decorations": true,
        "alwaysOnTop": false,
        "url": "dev-tools.html"
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; connect-src 'self' ws://localhost:* http://localhost:* https://api.openai.com https://api.anthropic.com; img-src 'self' data: https:;",
      "devCsp": null,
      "freezePrototype": false,
      "dangerousDisableAssetCspModification": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/tray-icon.png"
    ],
    "publisher": "AutoDev-AI Team",
    "copyright": "Copyright (c) 2024 AutoDev-AI Team",
    "category": "DeveloperTool",
    "shortDescription": "Neural Bridge Platform for AI Development",
    "longDescription": "Advanced neural bridge platform connecting Claude-Flow and OpenAI Codex for autonomous development workflows",
    "license": "MIT",
    "licenseFile": "../LICENSE",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "tsp": false,
      "wix": {
        "language": "en-US",
        "template": "main.wxs"
      },
      "webviewInstallMode": {
        "type": "downloadBootstrapper"
      },
      "allowDowngrades": true
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "externalBin": [],
    "resources": ["../resources/*"]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://releases.autodev-ai.com/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXkgZm9yIEF1dG9EZXYtQUkK"
    }
  }
}